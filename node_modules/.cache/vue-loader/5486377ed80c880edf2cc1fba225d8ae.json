esponse.data.items))\n      .catch((e) => {\n        this.errors.push(e);\n      });\n  },\n}; ///response.data.items[0].family\n</script>\n\n<style scoped>\n#fontCards {\n  margin-left: 20%;\n  margin-right: 20%;\n  margin-top: 1vw;\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n}\n\n#eachFont {\n  width: 15vw;\n  height: 15vw;\n  display: inline-block;\n  padding: 0.5rem;\n  order: 4;\n}\n\n.fontName {\n  position: relative;\n  top: 0%;\n  border-top: 1px solid darkslategrey;\n  border-bottom: 1px solid darkslategrey;\n  color: darkslategrey;\n  margin-top: 0.5vw;\n  margin-bottom: 1vw;\n}\n.fa-plus-circle {\n  position: relative;\n  left: 75%;\n  font-size: 13px;\n}\n.fontType {\n  margin-top: 0.5vw;\n  text-align: right;\n  color: darkslategrey;\n  margin-bottom: 1.5vw;\n}\n.sampleText {\n  color: darkslategrey;\n  word-spacing: 5px;\n  letter-spacing: 1px;\n  margin-top: 1.5vw;\n  margin-bottom: 2vw;\n}\n</style>\n"]}]}